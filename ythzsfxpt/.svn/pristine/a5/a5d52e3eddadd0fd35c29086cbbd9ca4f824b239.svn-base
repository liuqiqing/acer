@import "param";


.label-content{
    display: none;
    position: absolute;
    left: 35px;
    top: 10px;
    width: 120px;
    height: 200px;
    .platform-block{
        width: 120px;
        height: 120px;
        padding-top: 25px;
        margin: 20px auto 0;
        background: url(../img/index/share-platform-list.png) center no-repeat;
        .pb-icon{
            width: 24px;
            height: 24px;
            margin: 0 auto 10px;
            &.icon-1{
                background: url(../img/index/icon-1.png) center no-repeat;
            }
        }
        .pb-num{
            width: 100%;
            line-height: 44px;
            text-align: center;
            .num{
                color: #fff;
                margin: 0 auto;
                font-size: 24px;
                vertical-align: -2px;
                text-shadow: 0 0 15px #008cf8;
            }
        }
    }

}
.tra-chart{
    display: none;
    left: 35px;
    top: 10px;
    position: absolute;
    width: 380px;
    height: 180px;

}
.data-total{
    display: none;
    position: absolute;
    left: 35px;
    top: 10px;
    width: 330px;
    .data-title{
        height: 30px;
        background: url("../img/index/title-icon.png") center left no-repeat;
        color: #fff;
        text-indent:20px;
        .data-num{
            float: right;
            display: inline-block;
            border: 1px dashed #0183d1;
            height: 100%;
            line-height: 30px;
            padding: 0 4px;
            color: #fff;
            border-radius: 4px;
        }
    }
    .data-content{
        margin-top: 8px;
        ul{
            li{
                float: right;
                color: #fff;
                font-size: 14px;
                &+li{
                    margin-right: 21px;
                }
                &.content1{
                    height: 100px;
                    width: 86px;
                    text-align: center;
                    border: 1px solid #0096ff;
                    background-color: rgba(1,131,209,0.2);
                }
                &.content2{
                    height: 100px;
                    width: 86px;
                    text-align: center;
                    border: 1px solid #ffcf00;
                    background-color: rgba(255,207,0,0.2);
                }
                &.content3{
                    height: 100px;
                    width: 86px;
                    text-align: center;
                    border: 1px solid #796cfc;
                    background-color: rgba(121,108,252,0.2);
                }
                div{
                    margin-top: 8px;
                }
                .data-icon{
                    height: 34px;
                    &.ic1{
                        background: url("../img/index/ic1.png") center no-repeat;
                    }
                    &.ic2{
                        background: url("../img/index/ic2.png") center no-repeat;
                    }
                    &.ic3{
                        background: url("../img/index/ic3.png") center no-repeat;
                    }
                }

            }
        }
    }
}


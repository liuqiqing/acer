.label-content {
  display: none;
  position: absolute;
  left: 35px;
  top: 10px;
  width: 120px;
  height: 200px;
}
.label-content .platform-block {
  width: 120px;
  height: 120px;
  padding-top: 25px;
  margin: 20px auto 0;
  background: url(../img/index/share-platform-list.png) center no-repeat;
}
.label-content .platform-block .pb-icon {
  width: 24px;
  height: 24px;
  margin: 0 auto 10px;
}
.label-content .platform-block .pb-icon.icon-1 {
  background: url(../img/index/icon-1.png) center no-repeat;
}
.label-content .platform-block .pb-num {
  width: 100%;
  line-height: 44px;
  text-align: center;
}
.label-content .platform-block .pb-num .num {
  color: #fff;
  margin: 0 auto;
  font-size: 24px;
  vertical-align: -2px;
  text-shadow: 0 0 15px #008cf8;
}
.tra-chart {
  display: none;
  left: 35px;
  top: 10px;
  position: absolute;
  width: 380px;
  height: 180px;
}
.data-total {
  display: none;
  position: absolute;
  left: 35px;
  top: 10px;
  width: 330px;
}
.data-total .data-title {
  height: 30px;
  background: url("../img/index/title-icon.png") center left no-repeat;
  color: #fff;
  text-indent: 20px;
}
.data-total .data-title .data-num {
  float: right;
  display: inline-block;
  border: 1px dashed #0183d1;
  height: 100%;
  line-height: 30px;
  padding: 0 4px;
  color: #fff;
  border-radius: 4px;
}
.data-total .data-content {
  margin-top: 8px;
}
.data-total .data-content ul li {
  float: right;
  color: #fff;
  font-size: 14px;
}
.data-total .data-content ul li + li {
  margin-right: 21px;
}
.data-total .data-content ul li.content1 {
  height: 100px;
  width: 86px;
  text-align: center;
  border: 1px solid #0096ff;
  background-color: rgba(1, 131, 209, 0.2);
}
.data-total .data-content ul li.content2 {
  height: 100px;
  width: 86px;
  text-align: center;
  border: 1px solid #ffcf00;
  background-color: rgba(255, 207, 0, 0.2);
}
.data-total .data-content ul li.content3 {
  height: 100px;
  width: 86px;
  text-align: center;
  border: 1px solid #796cfc;
  background-color: rgba(121, 108, 252, 0.2);
}
.data-total .data-content ul li div {
  margin-top: 8px;
}
.data-total .data-content ul li .data-icon {
  height: 34px;
}
.data-total .data-content ul li .data-icon.ic1 {
  background: url("../img/index/ic1.png") center no-repeat;
}
.data-total .data-content ul li .data-icon.ic2 {
  background: url("../img/index/ic2.png") center no-repeat;
}
.data-total .data-content ul li .data-icon.ic3 {
  background: url("../img/index/ic3.png") center no-repeat;
}

@media screen and (min-width: 4000px) {
  .m-bottom-section {
    height: 260px;
  }
  .m-bottom-section ul li .panel-body {
    height: 220px;
  }
  .m-right-section {
    width: 650px;
  }
  .m-bottom-section ul li .panel-body #card-type1,
  .m-bottom-section ul li .panel-body #card-type2 {
    height: 60px;
  }
  .m-bottom-section ul li .panel-body .legal-people {
    width: 100%;
    height: 88px;
    line-height: 78px;
    letter-spacing: 1px;
    font-size: 24px;
  }
  .m-bottom-section ul li .panel-body .legal-people i {
    font-size: 28px!important;
  }
  .m-bottom-section ul li .gradient-item {
    height: 100px;
    line-height: 100px;
    font-size: 32px;
    padding-top: 0;
  }
  .m-bottom-section ul li .white {
    margin-top: 20px;
    font-size: 28px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list .pic-num-left {
    margin-left: 150px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list .pic-num-right {
    width: 390px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list li {
    margin-top: 25px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list {
    font-size: 24px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list .pic-num-right .pic-num-right-txt {
    margin-bottom: 10px;
  }
  .m-right-section .data-calling-list .panel-body .pic-num-list .pic-num-right .num {
    font-size: 42px;
  }
  .m-right-section .num-list {
    height: auto;
  }
  .m-right-section .num-list ul li {
    width: 65px;
    height: 80px;
    line-height: 80px;
    font-size: 32px;
  }
  .m-right-section .data-dire-list {
    height: 140px;
  }
  .m-right-section .use-ball-list .panel-body {
    text-align: center;
  }
  .m-right-section .use-ball-list .panel-body ul li {
    font-size: 30px;
  }
  .m-right-section .use-ball-list .panel-body ul li .ball-name {

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.inspur.dao.dzjc.dzjcMapper">

    <!-- 济南市 红牌数量 CUR_RESULT_TYPE = 5  -->
    <select id="hongpai" resultType="java.lang.Integer">
       select count(*) sl from EA_JC_BUSIINDEX where CUR_RESULT_TYPE = 5 and DONETIME is not null and REGION_ID = '370100'
    </select>
    <!-- 济南市 黄牌数量 CUR_RESULT_TYPE = 2  -->
    <select id="huangpai" resultType="java.lang.Integer">
       select count(*) sl from EA_JC_BUSIINDEX where CUR_RESULT_TYPE= 2 and DONETIME is not null and REGION_ID = '370100'
    </select>


    <!-- 获取网上评价的信息 scoreType 1是满意2是基本满意3是不满意-->
    <select id="getwspjxx" resultType="java.util.LinkedHashMap">
        SELECT COUNT(SCORETYPE) sum,SCORETYPE  FROM EA_JC_EVALUATE WHERE to_char(OCCURTIME,'yyyy-mm') = '2019-11' GROUP BY SCORETYPE
    </select>


        color: #fff;
      }
      em{
        font-size: 16px;
        color: #51ffff;
      }
    }
  }
  .flex-list.hori-list{
    height: auto;
    &> li + li{
      margin-left: 25px;
    }
    &.center-list{
      padding: 0 10px;
      margin-top: 40px;
      &> li + li{
        margin-left: 0;
      }
      .data-center,
      .busi-center{
        width: 110px;
        height: 112px;
        line-height: 112px;
        color: #fff;
        span{
          display: block;
          transform: rotateY(35deg);
          &:before,
          &:after{
            content: ' : ';
            font-weight: bold;
          }
        }
      }
      .data-center{
        padding-right: 10px;
        text-align: right;
        background: url("../img/countyLevel/data-center.png") no-repeat center;
      }
      .busi-center{
        padding-left: 10px;
        background: url("../img/countyLevel/busi-center.png") no-repeat center;
      }
    }
  }
  .line-center{
    position: relative;
    .line-bg{
      position: absolute;
      left: -10px;
      right: -10px;
      top: 0;
      bottom: 0;
      height: 28px;
      margin: auto 0;
      .line-blue,
      .line-orange{
        content: '';
        display: block;
        position: absolute;
        left: 0;
        width: 100%;
        height: 8px;
        &::after{
          content: '';
          display: block;
          position: absolute;
          top: 0;
          width: 57px;
          height: 8px;
        }
      }
      .line-orange{
        top: 0;
        background-color: rgba(252, 181, 79, .2);
        &::after{
          right: 0;
          background: url("../img/countyLevel/line-orange.png") no-repeat center;
          animation: line-orange 3s linear infinite;
        }
        .num-thin{
          position: absolute;
          width: 100%;
          bottom: 100%;
          margin-bottom: 10px;
          color: #f5b451;
          text-align: center;
        }
      }
      .line-blue{
        bottom: 0;
        background-color: rgba(23, 151, 239, .2);
        &::after{
          left: 0;
          background: url("../img/countyLevel/line-blue.png") no-repeat center;
          animation: line-blue 3s linear infinite;
        }
        .num-thin{
          position: absolute;
          width: 100%;
          top: 100%;
          margin-top: 10px;
          color: #1d8fdd;
          text-align: center;
        }
      }
    }
  }
}

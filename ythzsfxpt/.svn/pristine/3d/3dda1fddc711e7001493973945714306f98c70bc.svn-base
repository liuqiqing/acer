#app{
  position: relative;
  width: 1920px;
  height: 1080px;
  margin: auto;
  padding-top: 120px;
  padding-bottom: 0;
  font-size: 14px;
  font-family: "Helvetica Neue", Helvetica, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothingpanel-body: grayscale;
  background: url("../img/common/bg.jpg") #02050c no-repeat center top;
  overflow: hidden;
}
#header{
  position: absolute;
  z-index: 3;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 120px;
  background: url("../img/common/hdbg.png") no-repeat center top;
  img{
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    top: 25px;
    margin: 0 auto;
  }
  .header-main{
    position: relative;
    width: 720px;
    height: 90px;
    margin: 0 auto;
    background: url("../img/common/title.png") no-repeat center;
  }
}
#nav{
  position: absolute;
  z-index: 4;
  left: -220px;
  top: 0;
  width: 240px;
  height: 100%;
  padding-top: 150px;
  padding-bottom: 30px;
  background-color: rgba(0, 11, 32, .9);
  box-shadow: 5px 0 10px 0 rgba(12, 155, 255, .3);
  transition: .5s;
  cursor: pointer;
  &::after{
    content: '';
    position: absolute;
    z-index: 100;
    left: 0;
    top: 0;
    display: block;
    width: 100%;
    height: 100%;
  }
  .nav-toggle{
    position: absolute;
    left: 0;
    top: 60px;
    width: 100%;
    height: 40px;
    padding: 0 4px;
    line-height: 40px;
    text-align: right;
    overflow: hidden;
    cursor: pointer;
    .nav-arrow{
      display: inline-block;
      width: 12px;
      height: 18px;
      background: url("../img/common/navarrow.png") no-repeat center;
      vertical-align: middle;
      transition: .5s;
    }
    span{
      display: none;
      font-size: 16px;
      color: #00d8ff;
      vertical-align: middle;
    }
  }
  .nav-main{
    width: 100%;
    height: 100%;
  }
  .nav-title{
    height: 52px;
    line-height: 52px;
    font-size: 18px;
    text-align: center;
    color: #fff;
    overflow: hidden;
  }
  .nav-list{
    padding: 10px 0 30px;
    &>ul{
      &>li{
        position: relative;
        font-size: 16px;
        text-align: center;
        margin-left: 30px;
        width: 180px;
        border-bottom: 2px solid #162651;
        a{
          position: relative;
          display: inline-block;
          width: 160px;
          height: 52px;
          padding: 0 15px;
          line-height: 52px;
          color: #ddd;
          &:hover{
            color: #fff;
          }
        }
        &.active{
          /*&::before{
              content: '';
              position: absolute;
              right: 4px;
              top: 21px;
              display: block;
              width: 10px;
              height: 10px;
              border: 2px solid #0cdeff;
              border-radius: 50%;
          }*/
          a{
            color: #00d8ff;
            &::before,
            &::after{
              content: '';
              position: absolute;
              top: 21px;
              width: 70px;
              height: 10px;
            }
            &::before{
              right: 100%;
            }
            &::after{
              left: 100%;
              transform: rotateZ(180deg);
            }
          }
        }

      }
    }
  }
  &.active{
    left: 0;
    width: 240px;
    cursor: default;
    &::after{
      display: none;
    }
    .nav-toggle{
      text-align: center;
      .nav-arrow{
        transform: rotateZ(-180deg);
      }
      span{
        display: inline;
        margin-left: 15px;
      }
    }
    .nav-title{
      background: url("../img/common/line.png") no-repeat top;
    }
    .nav-list{
      &>ul{
        &>li{
          &.active{
            &::before{
              display: none;
            }
            a{
              &::after{
                background: url("../img/common/dian.png") no-repeat right;
              }
            }
          }

        }
      }
    }
  }
}

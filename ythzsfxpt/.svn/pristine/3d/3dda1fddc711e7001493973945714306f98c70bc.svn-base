@keyframes line-orange {
  0%{
    right: 0;
  }
  50%{
    opacity: 1;
  }
  100%{
    right: 70%;
    opacity: 0;
  }
}
@keyframes line-blue {
  0%{
    left: 0;
  }
  50%{
    opacity: 1;
  }
  100%{
    left: 70%;
    opacity: 0;
  }
}
@keyframes back-rotate{
  0%{
    transform: rotate(0deg);
  }
  100%{
    transform: rotate(360deg);
  }
}
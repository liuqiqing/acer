.panel{
  position: relative;
  padding-left: 40px;
  &::before,
  &::after{
    content: '';
    position: absolute;
    top: 0;
    display: block;
    width: 11px;
    height: 11px;
    border: 1px solid #1e5de3;
    border-bottom-width: 0;
  }
  &::before{
    left: 40px;
    border-right-width: 0;
  }
  &::after{
    right: 0;
    border-left-width: 0;
  }
  .panel-header{
    position: absolute;
    left: 0;
    top: 0;
    width: 40px;
    height: 100%;
    padding: 15px 0;
    line-height: 40px;
    background-color: rgba(0, 0, 0, .4);
    writing-mode: vertical-lr;
    writing-mode: tb-lr;
    box-shadow: 3px 0 3px 0 rgba(255, 255, 255, .05);
    &.blue{
      span::before{
        background-color: #1e5de3;
      }
    }
    &.yellow{
      span::before{
        background-color: #e3ca1e;
      }
    }
    &.green{
      span::before{
        background-color: #42ca83;
      }
    }
    span{
      letter-spacing: 3px;
      font-size: 18px;
      color: #fff;
      &::before{
        content: '';
        display: inline-block;
        width: 18px;
        height: 4px;
        margin-bottom: 15px;
        border-radius: 4px;
      }
    }
    i{
      float: right;
      font-size: 12px;
      color: #888;
    }
  }
  .panel-body{
    position: relative;
    width: 100%;
    height: 100%;
    background: url("../img/countyLevel/panelbg.png") repeat;
    &.panel-padding1{
      width: 100%;
      height: 100%;
      padding: 1px;
    }
    &.panel-padding{
      padding: 15px;
    }
    &::before,
    &::after{
      content: '';
      position: absolute;
      bottom: 0;
      display: block;
      width: 11px;
      height: 11px;
      border: 1px solid #1e5de3;
      border-top-width: 0;
    }
    &::before{
      left: 0;
      border-right-width: 0;
    }
    &::after{
      right: 0;
      border-left-width: 0;
    }
  }
  &.panel-hori{
    padding-left: 0;
    padding-top: 40px;
    &::before,
    &::after{
      top: 40px;
    }
    &::before{
      left: 0;
    }
    .panel-header{
      left: 0;
      top: 0;
      width: 100%;
      height: 40px;
      padding: 0 15px;
      line-height: 40px;
      text-align: center;
      writing-mode: lr;
      box-shadow: 0 3px 3px 0 rgba(255, 255, 255, .05);
      span::before{
        width: 4px;
        height: 18px;
        margin-right: 15px;
        margin-bottom: 0;
        vertical-align: middle;
      }
    }
  }
  &.panel-no-header{
    padding-left: 0;
    padding-top: 0;
    &::before,
    &::after{
      top: 0;
    }
  }
}

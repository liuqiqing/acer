.table-panel{
  .table-content{
    height: 600px;overflow: hidden;
    .table {
      width: 100%;
      border: none;
      border-collapse: collapse;
      thead {
        color: #fff;
        background-color: rgba(8, 16, 138, 0.15);
        box-shadow: inset 0 0 10px 3px rgba(6, 102, 232, 0.35);
        th, td {
          padding: 20px 8px;
          text-align: center;
        }
      }
      tbody {
        color: #fff;
        tr:nth-child(odd) {
          background-color: rgba(23, 64, 136, .2);
        }
        tr.fail {
          color: #cd530c;
        }
        th, td {
          padding: 5px 8px;
          text-align: center;
          line-height: 1.5;
          span {
            display: block;
            overflow: hidden;
            white-space: nowrap;
            text-overflow: ellipsis;
          }
        }
      }
    }
  }
}
// 数据运营
.operate-panel{
  .data-list{
    padding-top: 10px;
    &>ul>li{
      display: flex;
      height: 42px;
      padding: 0 10px;
      line-height: 40px;
      justify-content: space-between;
      &+li{

.tool-tip-container{
  position: relative;
  width: 160px;
  height: 180px;
  border: 1px dashed #0096ff;
  background-color: rgba(0,0,0,.8);
  &.tool-line{
    position: relative;
    &:before{
      content: '';
      position: absolute;
      width: 72px;
      height: 117px;
      left: -50px;
      top: 20px;
      background: url('../img/countyLevel/tooltip-line.png') center no-repeat;
    }
  }
  &:before{
    content: '';
    position: absolute;
    width: 132px;
    height: 8px;
    left: -2px;
    top: -2px;
  }
  &:after{
    content: '';
    position: absolute;
    width: 132px;
    height: 8px;
    left: -2px;
    bottom: -2px;
    -moz-transform: scaleY(-1);
    -webkit-transform: scaleY(-1);
    -o-transform: scaleY(-1);
    transform: scaleY(-1);
  }
  .tool-name{
    width: 100%;
    margin-top: 15px;
    text-align: center;
    font-size: 18px;
    color: #3cc5ff;
  }
  .tool-num{
    width: 100%;
    margin-top: 15px;
    text-align: center;
    font-size: 14px;
    .num{
      font-size: 24px;
      vertical-align: -2px;
      text-shadow: 0 0 10px #008cf8;
    }
  }
  .tool-catalog{
    width: 100%;
    margin-top: 5px;
    overflow: hidden;
    text-align: center;
    font-size: 14px;
    color: #fff;
    &.tool-sjml{
      margin-bottom: 15px;
    }
    .cata-type{
      float: left;
      margin-left: 20px;
    }
    .cata-num{
      width: 55px;
      float: right;
      text-align: right;
      margin-right: 20px;
      overflow: hidden;
      text-overflow:ellipsis;
      white-space: nowrap;
      &.kb-num{
        color: #1b54cf;
      }
      &.jk-num{
        color: #f5cf41;
      }
      &.wj-num{
        color: #f54141;
      }
    }
  }
}

.width-loop(@n, @i:0) when (@i <= @n) {
  .w-@{i} {
    width: @i * @width_unit;
  }
  .left-@{i}{
    left: @i * @width_unit;
  }
  .right-@{i}{
    right: @i * @width_unit;
  }
  .width-loop(@n, (@i + 1));
}
.height-loop(@n, @i:0) when (@i <= @n) {
  .h-@{i} {
    height: @i * @height_unit;
  }
  .top-@{i}{
    top: @i * @height_unit;
  }
  .bottom-@{i}{
    bottom: @i * @height_unit;
  }
  .height-loop(@n, (@i + 1));
}
.width-loop(48);
.height-loop(24);
.flex-list{
  display: flex;
  width: 100%;
  height: 100%;
  .flex-loop(@n, @i:0) when (@i <= @n) {
    .flex-@{i} {
      flex: @i;
    }
    .flex-loop(@n, (@i + 1));
  }
  .flex-loop(48);
  &>li{
    flex: 1;
  }
  &.hori-list{
    flex-direction: row;
    &>li{
      &+li{
        margin-left: @panel_unit;
      }
    }
  }
  &.vert-list{
    flex-direction: column;
    &>li{
      &+li{
        margin-top: @panel_unit;
      }
    }
  }
}

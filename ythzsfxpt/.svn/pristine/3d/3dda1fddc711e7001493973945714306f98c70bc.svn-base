// 事项性质数以及网上办理数：已发布、可网上办理数
.travel-panel{
  position: relative;
  .panel-body{
    padding: 10px;
  }
  table{
    table-layout: fixed;
    width: 100%;
    border:0;
    margin: 0;
    line-height: 1.5;
    font-size: 12px;
    color: #fff;
    border-collapse: collapse;
    thead{
      &::before{
        content: '';
        position: absolute;
        display: block;
        width: 100%;
        height: 2px;
        left: 0;
        top: 46px;
        background-image: linear-gradient(to right, transparent 5%, #073774 50%, transparent 95%);
      }
      &>tr{
        &>th{
          text-align: center;
        }
      }
    }
    tbody{
      &>tr{
        &>td{
          padding: 6px;
          text-align: center;
          overflow: hidden;
          text-overflow:ellipsis;
          white-space: nowrap;
          &.top-three{
            padding: 0;
          }
          em{
            color: #51ffff;
          }
          img{
            width: 20px;
            height: 23px;
            margin-top: 8px;
            margin-left: 4px;
          }
          .num-thin{
            font-size: 16px;
          }
          .bar-container{
            width: 100%;
            height: 4px;
            border-radius: 2px;
            background-color: rgba(62,58,71,.6);
            .bar-percentage{
              height: 100%;
              border-radius: 2px;
              background-color: #9e9ca3;
              transform-origin: 0;
              animation: barWidth 1s 1 ease-in-out;
              &:hover{
                background-color: #dcdbe0;
              }
              &.first{
                background-color: #f5cf41;
                &:hover{
                  background-color: #ffe278;
                }
              }
              &.second{
                background-color: #e2e3e3;
                &:hover{
                  background-color: #fff;
                }
              }
              &.third{
                background-color: #ea9e54;
                &:hover{
                  background-color: #edb987;
                }
              }
            }
            @keyframes barWidth {
              0%{transform: scaleX(0);}
              100%{transform: scaleX(1);}
            }
          }
        }
      }
    }
  }
}

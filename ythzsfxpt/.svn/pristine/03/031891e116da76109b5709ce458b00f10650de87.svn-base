#app {
  position: relative;
  width: 1920px;
  height: 1080px;
  margin: auto;
  padding-top: 120px;
  padding-bottom: 0;
  font-size: 14px;
  font-family: "Helvetica Neue", Helvetica, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothingpanel-body: grayscale;
  background: url("../img/common/bg.jpg") #02050c no-repeat center top;
  overflow: hidden;
}
#header {
  position: absolute;
  z-index: 3;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 120px;
  background: url("../img/common/hdbg.png") no-repeat center top;
}
#header img {
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 25px;
  margin: 0 auto;
}
#header .header-main {
  position: relative;
  width: 720px;
  height: 90px;
  margin: 0 auto;
  /*background: url("../img/common/title.png") no-repeat center;*/
  text-align: center;
  font-size: 30px;
  line-height: 90px;
  color: #ffff
}
#nav {
  position: absolute;
  z-index: 4;
  left: -220px;
  top: 0;
  width: 240px;
  height: 100%;
  padding-top: 150px;
  padding-bottom: 30px;
  background-color: rgba(0, 11, 32, 0.9);
  box-shadow: 5px 0 10px 0 rgba(12, 155, 255, 0.3);
  transition: 0.5s;
  cursor: pointer;
}
#nav::after {
  content: '';
  position: absolute;
  z-index: 100;
  left: 0;
  top: 0;
  display: block;
  width: 100%;
  height: 100%;
}
#nav .nav-toggle {
  position: absolute;
  left: 0;
  top: 60px;
  width: 100%;
  height: 40px;
  padding: 0 4px;
  line-height: 40px;
  text-align: right;
  overflow: hidden;
  cursor: pointer;
}
#nav .nav-toggle .nav-arrow {
  display: inline-block;
  width: 12px;
  height: 18px;
  background: url("../img/common/navarrow.png") no-repeat center;
  vertical-align: middle;
  transition: 0.5s;
}
#nav .nav-toggle span {
  display: none;
  font-size: 16px;
  color: #00d8ff;
  vertical-align: middle;
}
#nav .nav-main {
  width: 100%;
  height: 100%;
  /*overflow: unset !important;*/
}
#nav .nav-title {
  height: 52px;
  line-height: 52px;
  font-size: 18px;
  text-align: center;
  color: #fff;
  overflow: hidden;
}
#nav .nav-list {
  padding: 10px 0 30px;
}
#nav .nav-list > ul > li {
  position: relative;
  font-size: 16px;
  text-align: center;
  /*margin-left: 30px;*/
  /*width: 180px;*/
  border-bottom: 2px solid #162651;
}
#nav .nav-list > ul > li a {
  position: relative;
  display: inline-block;
  /*width: 160px;*/
  height: 52px;
  padding: 0 15px;
  line-height: 52px;
  color: #ddd;
}
#nav .nav-list > ul > li a:hover {
  color: #fff;
}
#nav .nav-list > ul > li.active {
  /*&::before{
                        content: '';
                        position: absolute;
                        right: 4px;
                        top: 21px;
                        display: block;
                        width: 10px;
                        height: 10px;
                        border: 2px solid #0cdeff;
                        border-radius: 50%;
                    }*/
}
#nav .nav-list > ul > li.active a {
  color: #00d8ff;
}
#nav .nav-list > ul > li.active a::before,
#nav .nav-list > ul > li.active a::after {
  content: '';
  position: absolute;
  top: 21px;
  width: 70px;
  height: 10px;
}
#nav .nav-list > ul > li.active a::before {
  right: 100%;
}
#nav .nav-list > ul > li.active a::after {
  left: 100%;
  transform: rotateZ(180deg);
}
#nav.active {
  left: 0;
  width: 240px;
  cursor: default;
}
#nav.active::after {
  display: none;
}
#nav.active .nav-toggle {
  text-align: center;
}
#nav.active .nav-toggle .nav-arrow {
  transform: rotateZ(-180deg);
}
#nav.active .nav-toggle span {
  display: inline;
  margin-left: 15px;
}
#nav.active .nav-title {
  background: url("../img/common/line.png") no-repeat top;
}
#nav.active .nav-list > ul > li.active::before {
  display: none;
}
#nav.active .nav-list > ul > li.active a::after {
  background: url("../img/common/dian.png") no-repeat right;
}
#main {
  position: relative;
  z-index: 2;
  width: 100%;
  height: 100%;
}
.panel {
  position: relative;
  width: 100%;
  height: 100%;
  padding-top: 40px;
}
.panel .panel-header {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 40px;
  line-height: 40px;
}
.panel .panel-header span {
  display: inline-block;
  padding-left: 25px;
  font-size: 18px;
  color: #dff2fd;
  background: url("../img/common/paneltitle.png") no-repeat 5px center;
}
.panel .panel-header > a {
  float: right;
  font-size: 14px;
  color: #0198df;
}
.login-box {
  position: absolute;
  top: 20px;
  right: 70px;
  color: #ffffff;
}
.login-box i {
  padding: 0 13px;
  font-size: 20px;
}
.login-box span {
  cursor: pointer;
}
#login {
  display: none;
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 1000;
  color: #ffffff;
}
#login .login-cont {
  width: 512px;
  height: 552px;
  background: url("../img/common/login-bg.png") center no-repeat;
  margin: 200px auto;
  position: relative;
}
#login .login-cont .login-title {
  font-size: 20px;
  text-align: center;
  line-height: 50px;
  padding-top: 17px;
}
#login .login-cont .login-input {
  padding-top: 45px;
}
#login .login-cont .login-input .input {
  width: 256px;
  height: 56px;
  line-height: 56px;
  background: #1a1a1a;
  border: 1px solid #018cff;
  border-radius: 5px;
  margin: 45px auto;
}
#login .login-cont .login-input .input i {
  font-size: 16px;
  padding: 0 20px;
  border-right: 1px solid #018cff;
}
#login .login-cont .login-input .input input {
  width: 190px;
  color: #ffffff;
  border: 0px;
  background-color: transparent;
  outline: none;
  padding: 0 15px;
}
#login .login-cont .login-input .input:hover {
  box-shadow: 0 0 15px 1px #018cff inset;
}
#login .login-cont .login-btn {
  width: 182px;
  height: 50px;
  border: 1px solid #018cff;
  border-radius: 5px;
  margin: 75px auto;
  text-align: center;
  line-height: 50px;
  font-size: 16px;
  letter-spacing: 5px;
  box-shadow: 0 0 15px 1px #018cff inset;
  cursor: pointer;
}
#login .login-cont .login-close {
  position: absolute;
  top: 35px;
  right: 35px;
  font-size: 24px;
  cursor: pointer;
}

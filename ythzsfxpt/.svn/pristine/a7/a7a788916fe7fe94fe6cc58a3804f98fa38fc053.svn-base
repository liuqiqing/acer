/*申请省级资源情况弹出框*/
.mask-panel {
  position: absolute;
  left: 0;
  right: 0;
  top: -20;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.94);
  opacity: 0;
  z-index: -99;
  transition: 0.5s;
  height: 1080px;
}
.mask-panel.active {
  opacity: 1;
  z-index: 99;
}
.mask-panel .box {
  display: none;
  position: relative;
  width: 1400px;
  height: 830px;
  margin: 100px auto;
  border: 1px solid #0d57ab;
  background-color: rgba(13, 87, 171, 0.56);
  padding: 50px;
}
.mask-panel .box.active {
  display: block;
}
.mask-panel .box .close-btn {
  position: absolute;
  width: 26px;
  height: 26px;
  right: 20px;
  text-align: center;
  line-height: 20px;
  top: 11px;
  border-radius: 50%;
  border: 3px solid #0096ff;
  cursor: pointer;
  font-size: 16px;
  color: #0096ff;
}
/*弹出框内容title*/
.mask-panel .table-content{
	height:100%;
	width:100%;
	overflow:auto;
}
.mask-panel .table-content .panel-header{
	width: 100%;
    height: 8%;
    
}
.mask-panel .table-content .panel-header span{
	line-height: 40px;
	font-size:18px;
	color:white;
}
/*滚动条样式*/
.mask-panel .table-content::-webkit-scrollbar { /*滚动条整体样式*/
	width: 4px; /*高宽分别对应横竖滚动条的尺寸*/
	height: 4px;
}

.mask-panel .table-content::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
	border-radius: 5px;
	-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
	background: rgba(7, 181, 221, 0.81);
}

.mask-panel .table-content::-webkit-scrollbar-track { /*滚动条里面轨道*/
	-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
	border-radius: 0;
	background: rgba(0, 0, 0, 0.1);
}

<script>
    // 登录操作
    $("#loginbtn").click(function () {
        var login = ctx + "/userlogin";
        $.ajax({
            type: "GET",
            async: true,
            url: login,
            data: {
                "account":$("#username").val(),
                "pwd":$("#password").val(),
                "appCode":"INSPUR-DZZW-YTHZS"
            },
            success: function (result) {
                if (result.resultUser.state == 200) {
                    array = result.resultUser.menus;
                    // console.log(array)
                    // 把获取到的菜单放到session里面
                    sessionStorage.setItem("array", JSON.stringify(array));
                    sessionStorage.setItem("username",result.resultUser.user.NAME);
                    sessionStorage.setItem("user",result.resultUser.user.ID);
                    window.location.href = 'egovIndex.html';
                }else if(result.resultUser.state == "3" || result.resultUser.state == "2" ){
                    console.log(result.resultUser.error);
                }else {
                    console.log(result.resultUser.state)
                }
            },
            error: function (errorMsg) {

            }
        });
    })
</script>

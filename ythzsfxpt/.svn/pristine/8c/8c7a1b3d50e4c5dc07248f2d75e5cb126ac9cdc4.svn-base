.m-left-section {
  float: right;
  margin-right: 40px;
  width: 1080px;
}
.m-left-section .info-section {
  float: right;
  width: 340px;
  height: 280px;
  border: 16px solid transparent;
  border-image: url("../img/example/section-bg.png") 16 16 fill;
}
.m-left-section .info-section.r-20 {
  margin-right: 20px;
}
.m-left-section .info-section.r-40 {
  margin-right: 40px;
}
.m-left-section .info-section.top-60 {
  margin-top: 40px;
}
.m-right-section {
  float: right;
  margin-right: 40px;
  width: 700px;
  height: 600px;
}
.m-right-section .table-content {
  height: 600px;
  overflow: hidden;
}
.m-right-section .table-content .table {
  width: 100%;
  border: none;
  border-collapse: collapse;
}
.m-right-section .table-content .table thead {
  color: #fff;
  background-color: rgba(8, 16, 138, 0.15);
  box-shadow: inset 0 0 10px 3px rgba(6, 102, 232, 0.35);
}
.m-right-section .table-content .table thead th,
.m-right-section .table-content .table thead td {
  padding: 20px 8px;
  text-align: center;
}
.m-right-section .table-content .table tbody {
  color: #fff;
}
.m-right-section .table-content .table tbody tr:nth-child(odd) {
  background-color: rgba(23, 64, 136, 0.2);
}
.m-right-section .table-content .table tbody tr.fail {
  color: #cd530c;
}
.m-right-section .table-content .table tbody th,
.m-right-section .table-content .table tbody td {
  padding: 5px 8px;
  text-align: center;
  line-height: 1.5;
}
.m-right-section .table-content .table tbody th span,
.m-right-section .table-content .table tbody td span {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.m-bottom-section {
  float: right;
  margin-right: 60px;
  margin-top: 40px;
  width: 1800px;
}
.m-bottom-section .info-section {
  float: right;
  width: 340px;
  height: 280px;
  border: 16px solid transparent;
  border-image: url("../img/example/section-bg.png") 16 16 fill;
}
.m-bottom-section .info-section.r-20 {
  margin-right: 20px;
}
.m-bottom-section .info-section.top-60 {
  margin-top: 30px;
}
.chart-content {
  width: 100%;
  height: 100%;
}
.panel-header {
  font-size: 14px;
  color: #01b7ff;
  float: left;
  margin-bottom: 10px;
}

.panel-header span {
  font-size: 18px;

}

.s-ranking {
  font-size: 18px;
}

.s-title {
  display: flex;
  flex-direction: row;
}

.s-ranking .s-title {
  width: 100%;
  height: 34px;
  line-height: 34px;
  margin-top: 5px;
  border-bottom: 1px #00c6ff solid;
  border-top: 1px #00c6ff solid;
  border-left: none;
  border-right: none;
  color: #00c6ff;
}

.s-ranking .s-title span {
  margin-left: 25px;
}

.panel-body {
  color: #fff;
  text-align: center;
}
.ball-icon{
  width: 125px;
  height: 84px;
  margin-top: 15px;
  margin-left: 20px;
  font-size: 18px;
  line-height: 84px;
  background: url('../../static/img/index/useball.png') no-repeat;
}
.ball-blue {
  background-position: -2px 0;
}
.ball-name {
  font-size: 18px;
}
@media screen and (min-width: 4000px) {
  .m-left-section {
    width: 2000px;
  }
  .m-left-section .info-section {
    width: 900px;
  }
  .m-right-section {
    width: 2000px;
  }
  .m-bottom-section {
    width: 4040px;
  }
  .m-bottom-section .info-section {
    width: 900px;
  }
}

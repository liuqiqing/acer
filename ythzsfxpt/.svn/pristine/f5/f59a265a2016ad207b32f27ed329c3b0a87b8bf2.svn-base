.panel {
  position: relative;
  padding-left: 40px;
}
.panel::before,
.panel::after {
  content: '';
  position: absolute;
  top: 0;
  display: block;
  width: 11px;
  height: 11px;
  border: 1px solid #1e5de3;
  border-bottom-width: 0;
}
.panel::before {
  left: 40px;
  border-right-width: 0;
}
.panel::after {
  right: 0;
  border-left-width: 0;
}
.panel .panel-header {
  position: absolute;
  left: 0;
  top: 0;
  width: 40px;
  height: 100%;
  padding: 15px 0;
  line-height: 40px;
  background-color: rgba(0, 0, 0, 0.4);
  writing-mode: vertical-lr;
  writing-mode: tb-lr;
  box-shadow: 3px 0 3px 0 rgba(255, 255, 255, 0.05);
}
.panel .panel-header.blue span::before {
  background-color: #1e5de3;

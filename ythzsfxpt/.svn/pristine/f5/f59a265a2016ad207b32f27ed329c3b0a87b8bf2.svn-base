#app {
   position: relative;
  width: 100%;
  height: 100%;
  min-height: 1080px;
  margin: auto;
  padding-top: 120px;
  padding-bottom: 0;
  font-size: 14px;
  font-family: "Helvetica Neue", Helvetica, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", Arial, sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothingpanel-body: grayscale;
  background: url("../img/common/bg.jpg") #02050c no-repeat center top;
  overflow: hidden;
}
#header {
  position: absolute;
  z-index: 3;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 120px;
  background: url("../img/common/hdbg.png") no-repeat center top;
}
#header img {
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 25px;
  margin: 0 auto;
}
#header .header-main {
  position: relative;
  width: 720px;
  height: 90px;
  margin: 0 auto;
  /*background: url("../img/common/title.png") no-repeat center;*/
  text-align: center;
  font-size: 30px;
  line-height: 90px;
  color: #ffff
}
#nav {
  position: absolute;
  z-index: 4;
  left: -220px;
  top: 0;
  width: 240px;
  height: 100%;
  padding-top: 150px;
  padding-bottom: 30px;
  background-color: rgba(0, 11, 32, 0.9);
  box-shadow: 5px 0 10px 0 rgba(12, 155, 255, 0.3);
  transition: 0.5s;
  cursor: pointer;
}
#nav::after {
  content: '';
  position: absolute;
  z-index: 100;
  left: 0;
  top: 0;
  display: block;
  width: 100%;
  height: 100%;
}
#nav .nav-toggle {
  position: absolute;
  left: 0;
  top: 60px;
  width: 100%;
  height: 40px;
  padding: 0 4px;
  line-height: 40px;
  text-align: right;
  overflow: hidden;
  cursor: pointer;
}
#nav .nav-toggle .nav-arrow {
  display: inline-block;
  width: 12px;
  height: 18px;
  background: url("../img/common/navarrow.png") no-repeat center;
  vertical-align: middle;
  transition: 0.5s;
}
#nav .nav-toggle span {
  display: none;
  font-size: 16px;
  color: #00d8ff;
  vertical-align: middle;
}
#nav .nav-main {
  width: 100%;
  height: 100%;
}
#nav .nav-title {
  height: 52px;
  line-height: 52px;
  font-size: 18px;
  text-align: center;
  color: #fff;
  overflow: hidden;
}
#nav .nav-list {
  padding: 10px 0 30px;
}
#nav .nav-list > ul > li {
  position: relative;
  font-size: 16px;
  text-align: center;
/*   margin-left: 30px;
  width: 180px;
  border-bottom: 2px solid #162651; */
}
#nav .nav-list > ul > li a {
  position: relative;
  display: inline-block;
  max-width: 100%;
  height: 52px;
  padding: 0 15px;
  line-height: 52px;
  color: #ddd;
}
#nav .nav-list > ul > li a:hover {
  color: #fff;
}
#nav .nav-list > ul > li.active {
  /*&::before{
              content: '';
              position: absolute;
              right: 4px;
              top: 21px;
              display: block;
              width: 10px;
              height: 10px;
              border: 2px solid #0cdeff;
              border-radius: 50%;
          }*/
}
#nav .nav-list > ul > li.active::before {
  content: '';
  position: absolute;
  right: 4px;
  top: 21px;
  display: block;
  width: 10px;
  height: 10px;
  border: 2px solid #0cdeff;
  border-radius: 50%;
}
#nav .nav-list > ul > li.active a {
  color: #00d8ff;
}
#nav .nav-list > ul > li.active a::before,
#nav .nav-list > ul > li.active a::after {
  content: '';
  position: absolute;
  top: 21px;
  width: 70px;
  height: 10px;
}
#nav .nav-list > ul > li.active a::before {
  right: 100%;
}
#nav .nav-list > ul > li.active a::after {
  left: 100%;
  transform: rotateZ(180deg);
}
#nav.active {
  left: 0;
  width: 240px;
  cursor: default;
}
#nav.active::after {
  display: none;
}
#nav.active .nav-toggle {
  text-align: center;
}
#nav.active .nav-toggle .nav-arrow {
  transform: rotateZ(-180deg);
}
#nav.active .nav-toggle span {
  display: inline;
  margin-left: 15px;
}
#nav.active .nav-title {
  background: url("../img/common/line.png") no-repeat top;
}
#nav.active .nav-list > ul > li.active::before {
  display: none;
}
#nav.active .nav-list > ul > li.active a::after {
  background: url("../img/common/dian.png") no-repeat right;
}
